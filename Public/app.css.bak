html, body {
  font-family: "\5FAE\8F6F\96C5\9ED1", "Helvetica Neue", Helvetica, Arial, sans-serif;
  height: 100%;
}
a {
  text-decoration: none;
}
a:link, a:visited, a:hover, a:active {
  text-decoration: none;
}
.react_div_warp {
  height: 100%;
  min-height: 100%;
}
.react_div_temp {
  min-height: 100%;
  margin-top: -60px;
  padding-top: 60px;
  padding-bottom: 223px;
  margin-bottom: -223px;
}
#app {
  height: 100%;
  min-height: 100%;
}
.header {
  background-color: #2ea7e0;
  height: 60px;
}
.header .sofnav {
  width: 1000px;
  margin: 0 auto;
  background-color: transparent;
  border: transparent;
  color: #fff;
}
.header .sofnav .navbar-header {
  height: 60px;
}
.header .sofnav .navbar-header .navbar-brand {
  color: #fff;
  height: 60px;
  line-height: 30px;
}
.header .sofnav .navbar-header .navbar-brand:hover {
  color: #eee;
}
.header .sofnav .navbar-collapse {
  height: 60px;
  line-height: 30px;
}
.header .sofnav .navbar-collapse .navbar-nav li a {
  height: 60px;
  line-height: 30px;
  color: #fff;
}
.header .sofnav .navbar-collapse .navbar-nav li a:hover {
  color: #eee;
}
.header .sofnav .navbar-collapse .navbar-nav li.splitLogin {
  line-height: 30px;
  padding: 15px 0;
}
.header .sofnav .navbar-collapse .navbar-nav li.active a {
  background-color: #3399cc;
  color: #eee;
}
.sof-modal {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 9191;
}
.sof-modal.mapDialog {
  width: 900px;
  height: 500px;
  margin: 40px auto;
}
.sof-modal.mapDialog .modal-content {
  width: 100%;
  height: 100%;
  background-color: #000;
}
.sof-modal.mapDialog .modal-content .map {
  width: 100%;
  height: 100%;
}
.sof-modal .choose-pane {
  position: relative;
  width: 1000px;
  min-height: 400px;
  margin: 100px auto;
  background-color: #fff;
  border: 1px solid #cfcfcf;
  z-index: 9999;
  overflow: hidden;
}
.sof-modal .choose-pane .next {
  background-color: #f1f1f1;
}
.sof-modal .choose-pane .next .btn {
  margin-top: 8px;
  margin-right: 10px;
}
.sof-modal .choose-pane .choose-title {
  width: 100%;
  height: 50px;
  line-height: 50px;
  background-color: #f1f1f1;
  overflow: hidden;
}
.sof-modal .choose-pane .choose-title .choose-times {
  padding-left: 20px;
}
.sof-modal .choose-pane .choose-title .choose-times > div {
  display: inline-block;
}
.sof-modal .choose-pane .choose-title .choose-times .added {
  margin-left: 20px;
}
.sof-modal .choose-pane .choose-title .choose-times .start-time, .sof-modal .choose-pane .choose-title .choose-times .report-type {
  display: inline-block;
}
.sof-modal .choose-pane .choose-title .choose-times .start-time label, .sof-modal .choose-pane .choose-title .choose-times .report-type label {
  font-weight: normal;
}
.sof-modal .choose-pane .choose-title .choose-times .start-time {
  padding-right: 20px;
}
.sof-modal .choose-pane .choose-title .choose-times .start-time input {
  width: 130px;
  height: 30px;
  line-height: initial;
}
.sof-modal .choose-pane .choose-title .choose-times .report-type select {
  width: 130px;
  height: 30px;
  line-height: initial;
}
.sof-modal .choose-pane .choose-title .choose-operation {
  margin-right: 20px;
  display: inline;
}
.sof-modal .choose-pane .choose-title .choose-operation .choose-btn {
  display: inline-block;
  cursor: pointer;
  color: #363636;
}
.sof-modal .choose-pane .choose-title .choose-operation .choose-btn:hover {
  color: #3399cc;
}
.sof-modal .choose-pane .choose-title .choose-operation .choose-btn.choose-btn-enter {
  margin-right: 10px;
}
.sof-modal .choose-pane .choose-content {
  width: 200%;
  min-height: 300px;
  -webkit-transition:all 0.5s;
  transition: all 0.5s;
}
.sof-modal .choose-pane .choose-content.step0 {
  -webkit-transform: translateX(0);
          transform: translateX(0);
}
.sof-modal .choose-pane .choose-content.step1 {
  -webkit-transform: translateX(-50%);
